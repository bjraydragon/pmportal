<!--pages/prjlist/prjlist.wxml-->
<view class="page">
  <view class="page__bd">
    <view class="weui-panel weui-panel_access">
      <view class="weui-panel__hd">项目列表</view>
      <view class="weui-panel__bd">
        <navigator url="../grade/grade" class="weui-media-box weui-media-box_appmsg" hover-class="weui-cell_active">
          <view class="weui-media-box__hd weui-media-box__hd_in-appmsg">
            <image class="weui-media-box__thumb" src="../../images/project.png" />
          </view>
          <view class="weui-media-box__bd weui-media-box__bd_in-appmsg">
            <view class="weui-media-box__title" wx:for="{{projectList}}">{{item.projectCode}}   </view>
            <view class="weui-media-box__desc" wx:for="{{projectList}}">{{item.stage}}</view>
          </view>
        </navigator>
        
      </view>
    </view>

  </view>
</view>